{
  "manifest_version": 3,
  "name": "Space.Tab",
  "version": "1.0.0",
  "description": "一个美观实用的新标签页替代品",
  "permissions": [
    "storage",
    "geolocation"
  ],
  "host_permissions": [
    "https://restapi.amap.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
